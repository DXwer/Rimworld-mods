    <!--  Projectiles  -->

    <ThingDef Name="BaseEccentric10x24mmChargedBullet" ParentName="Base6x18mmChargedBullet" Abstract="True">
        <thingClass>EccentricProjectiles.ProjectileCE_WithEffects</thingClass>
        <graphicData>
            <texPath>Things/Eccentric/Projectile/ChargeProjectile_Small</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>TransparentPostLight</shaderType>
        </graphicData>
        <modExtensions>
            <li Class="EccentricProjectiles.ModExtension_ProjectileEffects">
                <effects>
                    <li>Eccentric_ChargeMuzzleFlashSmall</li>
                </effects>
            </li>
        </modExtensions>
    </ThingDef>

    <!--Standard-->

    <ThingDef ParentName="BaseEccentric10x24mmChargedBullet">
        <defName>Bullet_Eccentric10x24mmCharged</defName>
        <label>10x24mm Charged bullet</label>
        <projectile Class="CombatExtended.ProjectilePropertiesCE">
            <damageAmountBase>14</damageAmountBase>
            <secondaryDamage>
                <li>
                    <def>Bomb_Secondary</def>
                    <amount>4</amount>
                </li>
            </secondaryDamage>
            <armorPenetrationSharp>14</armorPenetrationSharp>
            <armorPenetrationBlunt>19.4</armorPenetrationBlunt>
        </projectile>
    </ThingDef>

    <!--AP-->

    <ThingDef ParentName="BaseEccentric10x24mmChargedBullet">
        <defName>Bullet_Eccentric10x24mmCharged_AP</defName>
        <label>10x24mm Charged bullet (Conc.)</label>
        <projectile Class="CombatExtended.ProjectilePropertiesCE">
            <damageAmountBase>10</damageAmountBase>
            <secondaryDamage>
                <li>
                    <def>Bomb_Secondary</def>
                    <amount>2</amount>
                </li>
            </secondaryDamage>
            <armorPenetrationSharp>28</armorPenetrationSharp>
            <armorPenetrationBlunt>19.4</armorPenetrationBlunt>
        </projectile>
    </ThingDef>

    <!--Ion-->

    <ThingDef ParentName="BaseEccentric10x24mmChargedBullet">
        <defName>Bullet_Eccentric10x24mmCharged_Ion</defName>
        <label>10x24mm Charged bullet (Ion)</label>
        <projectile Class="CombatExtended.ProjectilePropertiesCE">
            <damageAmountBase>9</damageAmountBase>
            <secondaryDamage>
                <li>
                    <def>EMP</def>
                    <amount>7</amount>
                </li>
            </secondaryDamage>
            <armorPenetrationSharp>21</armorPenetrationSharp>
            <armorPenetrationBlunt>19.4</armorPenetrationBlunt>
            <empShieldBreakChance>0.2</empShieldBreakChance>
        </projectile>
    </ThingDef>

</Defs>