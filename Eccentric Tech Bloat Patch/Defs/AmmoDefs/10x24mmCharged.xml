<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <ThingCategoryDef>
        <defName>Ammo10x24mmCharged</defName>
        <label>10x24mm Charged</label>
        <parent>AmmoAdvanced</parent>
        <iconPath>UI/Icons/ThingCategories/CaliberCharge</iconPath>
    </ThingCategoryDef>

    <CombatExtended.AmmoSetDef>
        <defName>AmmoSet_10x24mmCharged</defName>
        <label>10x24mm Charged</label>
        <ammoTypes>
            <Ammo_10x24mmCharged>Bullet_Eccentric10x24mmCharged</Ammo_10x24mmCharged>
            <Ammo_10x24mmCharged_AP>Bullet_Eccentric10x24mmCharged_AP</Ammo_10x24mmCharged_AP>
            <Ammo_10x24mmCharged_Ion>Bullet_Eccentric10x24mmCharged_Ion</Ammo_10x24mmCharged_Ion>
        </ammoTypes>
    </CombatExtended.AmmoSetDef>

    <!--Standard-->

    <ThingDef Class="CombatExtended.AmmoDef" ParentName="SpacerSmallAmmoBase">
        <thingClass>CombatExtended.AmmoThing</thingClass>
        <defName>Ammo_10x24mmCharged</defName>
        <label>10x24mm Charged</label>
        <description>The standard pistol cartridge of the Eccentric.</description>
        <graphicData>
			<texPath>Things/Ammo/Charged/SmallRegular</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
        <statBases>
            <Mass>0.008</Mass>
			<Bulk>0.01</Bulk>
            <MarketValue>0.5</MarketValue>
        </statBases>
        <tradeTags>
            <li>CE_AutoEnableTrade</li>
            <li>CE_AutoEnableCrafting_FabricationBench</li>
        </tradeTags>
        <thingCategories>
            <li>Ammo10x24mmCharged</li>
        </thingCategories>
        <stackLimit>5000</stackLimit>
        <ammoClass>Charged</ammoClass>
    </ThingDef>

    <RecipeDef ParentName="AmmoRecipeBase">
        <defName>MakeAmmo_10x24mmCharged</defName>
        <label>make 10x24mm Charged cartridge x500</label>
        <description>Craft 500 10x24mm Charged cartridges.</description>
        <jobString>Making 10x24mm Charged cartridges.</jobString>
        <researchPrerequisite>Eccentric_MiniCharge</researchPrerequisite>
        <skillRequirements>
            <Crafting>7</Crafting>
        </skillRequirements>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Plasteel</li>
                    </thingDefs>
                </filter>
                <count>15</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Steel</li>
                    </thingDefs>
                </filter>
                <count>8</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>ComponentIndustrial</li>
                    </thingDefs>
                </filter>
                <count>6</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>Plasteel</li>
                <li>Steel</li>
                <li>ComponentIndustrial</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <Ammo_10x24mmCharged>500</Ammo_10x24mmCharged>
        </products>
        <workAmount>6600</workAmount>
    </RecipeDef>

